{
  "compilerOptions": {
    "incremental": true,
    "target": "es2015",
    "module": "umd",
    "outDir": ".temp",
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [
    "*.ts"
  ]
}
