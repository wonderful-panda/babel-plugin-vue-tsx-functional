{
  "name": "babel-plugin-vue-tsx-functional",
  "version": "0.1.0",
  "keywords": [
    "Vue",
    "TypeScript",
    "Tsx",
    "Babel"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --list-different src/*.ts test/*.ts",
    "test": "jest",
    "build-all": "npm-run-all prettier build test"
  },
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@types/babel__core": "^7.1.3",
    "@types/jest": "^24.0.18",
    "@vue/babel-preset-app": "^3.11.0",
    "ast-pretty-print": "^2.0.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "~3.5.1",
    "vue": "^2.6.10",
    "vue-tsx-support": "^2.3.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.6.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    }
  }
}
